{
    "name": "scoutapp/scout-apm-symfony-bundle",
    "description": "Scout Application Performance Monitoring Agent - https://scoutapm.com",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "require": {
        "php": ">=7.1.0,<7.5.0",
        "ext-json": "*",
        "psr/log": "^1.0",
        "scoutapp/scout-apm-php": "^3.1",
        "symfony/config": "^4.0 || ^5.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/event-dispatcher": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^7.0",
        "phpunit/phpunit": "^8.5",
        "psr/simple-cache": "^1.0",
        "vimeo/psalm": "^3.8"
    },
    "suggest": {
        "psr/simple-cache": "Set up a service for Psr\\SimpleCache\\CacheInterface to improve Scout performance"
    },
    "autoload": {
        "psr-4": {
            "Scoutapm\\ScoutApmBundle\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scoutapm\\ScoutApmBundle\\Tests\\Unit\\": "./test/Unit"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
